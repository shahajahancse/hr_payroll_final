            $('#profile_image').attr('src', hostname+'assets/uploads/'+ data.epm_info.img_source);
            $('#profile_image').attr('alt', hostname+'awedget/assets/img/avater.jpg');
            $('#emp_name').empty().text(data.epm_info.name_en);
            $('#leave_entitle_casual').empty().text(data.leave_entitle_casual);
            $('#leave_entitle_sick').empty().text(data.leave_entitle_sick);
            $('#leave_entitle_maternity').empty().text(data.leave_entitle_maternity);
            $('#leave_entitle_paternity').empty().text(data.leave_entitle_paternity);
            $('#leave_taken_casual').empty().text(data.leave_taken_casual);
            $('#leave_taken_sick').empty().text(data.leave_taken_sick);
            $('#leave_taken_maternity').empty().text(data.leave_taken_maternity);
            $('#leave_taken_paternity').empty().text(data.leave_taken_paternity);
            $('#leave_balance_casual').empty().text(data.leave_balance_casual);
            $('#leave_balance_sick').empty().text(data.leave_balance_sick);
            $('#leave_balance_maternity').empty().text(data.leave_balance_maternity);
            $('#leave_balance_paternity').empty().text(data.leave_balance_paternity);